{
	"name": "web-cat-vscode",
	"displayName": "Web-CAT Submitter",
	"description": "Submit assignments to the Web-CAT online grader without using Eclipse!",
	"version": "1.0.2",
	"publisher": "oliver-ni",
	"repository": {
		"type": "git",
		"url": "https://github.com/oliver-ni/web-cat-vscode.git"
	},
	"icon": "wc-logo.png",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:web-cat.snarf",
		"onCommand:web-cat.submit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "web-cat.snarf",
				"title": "Web-CAT: Open Snarfer Browser"
			},
			{
				"command": "web-cat.submit",
				"title": "Web-CAT: Submit Assignment"
			}
		],
		"configuration": {
			"title": "Web-CAT Submitter",
			"properties": {
				"web-cat.java-path": {
					"type": "string",
					"default": "java",
					"description": "Command for java (or path to java executable)."
				},
				"web-cat.snarf-url": {
					"type": "string",
					"description": "URL for snarfer.\nLHS IntroCS: http://205.173.41.10/pythonsnarf/snarf.xml\nLHS APCS: http://205.173.41.10/javasnarf/snarf.xml"
				},
				"web-cat.submit-url": {
					"type": "string",
					"description": "URL to submit to.\nLHS: http://205.173.41.10/Web-CAT/WebObjects/Web-CAT.woa/wa/assignments/eclipse"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/unzipper": "^0.10.2",
		"axios": "^0.19.2",
		"fast-xml-parser": "^3.16.0",
		"unzipper": "^0.10.10"
	}
}
